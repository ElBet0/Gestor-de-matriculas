<?xml version="1.0" encoding="utf-8"?>
<!--
  Para obtener más información sobre cómo configurar la aplicación ASP.NET, visite
  https://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1" />
    <httpRuntime targetFramework="4.8.1" />
	  <authentication mode="Forms">
		  <forms loginUrl="~/Login.aspx" timeout="30"/>
	  </authentication>
	  <authorization>
		  <deny users="?"/>
	  </authorization>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="FamiliaWSPortBinding" />
        <binding name="AlumnoWSPortBinding" />
        <binding name="UsuarioWSPortBinding" />
        <binding name="UsuarioWSPortBinding1" />
        <binding name="CursoWSPortBinding" />
        <binding name="GradoAcademicoWSPortBinding" />
        <binding name="AulaWSPortBinding" />
        <binding name="PersonalWSPortBinding" />
        <binding name="CargoWSPortBinding" />
        <binding name="PeriodoWSPortBinding" />
        <binding name="PeriodoXAulaWSPortBinding" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8080/SisProgWS/CursoWS" binding="basicHttpBinding"
        bindingConfiguration="CursoWSPortBinding" contract="SisProgWS.CursoWS"
        name="CursoWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/GradoAcademicoWS"
        binding="basicHttpBinding" bindingConfiguration="GradoAcademicoWSPortBinding"
        contract="SisProgWS.GradoAcademicoWS" name="GradoAcademicoWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/UsuarioWS"
        binding="basicHttpBinding" bindingConfiguration="UsuarioWSPortBinding"
        contract="SisProgWS.UsuarioWS" name="UsuarioWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/AulaWS" binding="basicHttpBinding"
        bindingConfiguration="AulaWSPortBinding" contract="SisProgWS.AulaWS"
        name="AulaWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/PeriodoWS"
        binding="basicHttpBinding" bindingConfiguration="PeriodoWSPortBinding"
        contract="SisProgWS.PeriodoWS" name="PeriodoWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/AlumnoWS"
        binding="basicHttpBinding" bindingConfiguration="AlumnoWSPortBinding"
        contract="SisProgWS.AlumnoWS" name="AlumnoWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/FamiliaWS"
        binding="basicHttpBinding" bindingConfiguration="FamiliaWSPortBinding"
        contract="SisProgWS.FamiliaWS" name="FamiliaWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/PersonalWS"
        binding="basicHttpBinding" bindingConfiguration="PersonalWSPortBinding"
        contract="SisProgWS.PersonalWS" name="PersonalWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/PeriodoXAulaWS"
        binding="basicHttpBinding" bindingConfiguration="PeriodoXAulaWSPortBinding"
        contract="SisProgWS.PeriodoXAulaWS" name="PeriodoXAulaWSPort" />
      <endpoint address="http://localhost:8080/SisProgWS/CargoWS" binding="basicHttpBinding"
        bindingConfiguration="CargoWSPortBinding" contract="SisProgWS.CargoWS"
        name="CargoWSPort" />
    </client>
  </system.serviceModel>
</configuration>